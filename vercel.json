{
  "framework": "nuxtjs",
  "buildCommand": "node scripts/vercel-build.js",
  "installCommand": "npm install",
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "functions": {
    "server/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["sin1"],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
